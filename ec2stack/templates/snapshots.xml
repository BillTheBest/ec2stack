{% extends "response.xml" %}
{% block response_content %}
    <snapshotSet>
        {% for snapshot in response.vmsnapshot %}
        <item>
            <snapshotId>{{ snapshot.id }}</snapshotId>
            <volumeId>{{ snapshot.volumeid }}</volumeId>
            <status>{{ response.state }}</status>
        </item>
        {% endfor %}
    </snapshotSet>
{% endblock %}