{% extends "response.xml" %}
{% block response_content %}
    <imagesSet>
        {% for image in images %}
        <item>
            <imageId>
            {{ image.id }}
            </imageId>
            <name>{{ image.name }}</name>
            <imageState>{{ image.state }}</imageState>
            <hypervisor>{{ image.hypervisor }}</hypervisor>
            <description>{{ image.description }}</description>
        </item>
        {% endfor %}
    </imagesSet>
{% endblock %}