{% extends "response.xml" %}
{% block response_content %}
    {% if reservation_id %}
    <reservationSet>
        <item>
            <reservationId>{{reservation_id}}</reservationId>
            <ownerId>{{owner_id}}</ownerId>
    {% endif %}
            <{{item_to_describe}}sSet>
            {% for item in items %}
            <item>
                <{{item_to_describe}}Id>
                {{item.id}}
                </{{item_to_describe}}Id>
                <name>{{item.name}}</name>
                <{{item_to_describe}}State>{{item.state}}</{{item_to_describe}}State>
            </item>
            {% endfor %}
        </{{item_to_describe}}sSet>
    {% if reservation_id %}
        </item>
        </reservationSet>
    {% endif %}
{% endblock %}