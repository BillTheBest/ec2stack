{% extends "response.xml" %}
{% block response_content %}
    <volumeSet>
        {% for volume in response.volume %}
        <item>
            <volumeId>
                {{ volume.id }}
            </volumeId>
            <name>{{ volume.name }}</name>
            <createTime>{{ volume.created }}</createTime>
            <attachmentSet>
                <item>
                    <volumeId>
                        {{ volume.id }}
                    </volumeId>
                    <instanceId>
                        {{ volume.virtualmachineid }}
                    </instanceId>
                </item>
            </attachmentSet>
        </item>
        {% endfor %}
    </volumeSet>
{% endblock %}